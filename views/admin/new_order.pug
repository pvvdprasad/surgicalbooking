    function validateform() {
        // Get values of required input fields
        var surgeryCenter = o('surgery_center').value;
        var startDate = o('datepicker').value;
        var firstName = o('pat_first_name').value;
        var middleName = o('pat_middle_name').value;
        var lastName = o('pat_last_name').value;
        var dob = o('datepicker2').value;
        var gender= o('selectgender').value;
        var side = o('select_side').value;
        var manu = o('select_manu').value;
        var brand = o('select_brand').value;
        var model = o('select_model').value;
        var power = o('select_power').value;
        var manu2 = o('select_manu2').value;
        var brand2 = o('select_brand2').value;
        var model2 = o('select_model2').value;
        var power2 = o('select_power2').value;
        console.log(gender);
        // Check if any required field is empty, null, or undefined
        if (
            !surgeryCenter || !startDate || !firstName || !lastName ||
            !dob || !side || !manu || !brand || !model || !power ||
            !manu2 || !brand2 || !model2 || !power2 || !gender
        ) {
            alert('Please fill out all required fields.'); // Alert user about the missing field(s)
            return; // Stop execution if any required field is missing
        }

        // If all required fields are present, proceed with save_order function
        save_order();
    }

